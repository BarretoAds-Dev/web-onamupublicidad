{
  "name": "onamu-publicidad-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "preview": "astro build && wrangler pages dev",
    "astro": "astro",
    "deploy": "wrangler pages deploy --main",
    "cf-typegen": "wrangler types",
    "github": "git add . && git commit -m 'Deploy to GitHub' && git push origin HEAD",
