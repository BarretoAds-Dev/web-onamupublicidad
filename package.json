    "build": "pnpm run clean && astro check && astro build && pnpm run github && pnpm run deploy"


  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/cloudflare": "^11.0.4",
    "astro": "^4.15.7",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "wrangler": "^3.78.4"
  }
}